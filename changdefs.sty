\DeclarePairedDelimiterX\braket[2]{\langle}{\rangle}{#1 \delimsize\vert #2} % \vert: auto-resizable; \mid: auto-spacing
\DeclarePairedDelimiterX\obraket[3]{\langle}{\rangle}{#1 \delimsize\vert #2 \delimsize\vert #3} % \vert: auto-resizable; \mid: auto-spacing
%% Swap the definition of \lrvert* and \lrVert*, so that \lrvert and \lrVert resizes the size of the brackets, and the starred version does not.
\let\origbra\bra
\def\bra{\@ifstar{\origbra}{\origbra*}}
\let\origket\ket
\def\ket{\@ifstar{\origket}{\origket*}}
\let\origbraket\braket
