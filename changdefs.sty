  \def\temp{#1} \ifx\temp\empty
  \frac{\ud #2}{\ud #3}
  \else
  \frac{\ud^{#1} #2}{{\ud #3}^{#1}}
  \fi
}
\newcommand{\fracdelta}[3][]{%
  \def\temp{#1} \ifx\temp\empty
  \frac{\delta #2}{\delta #3}
  \else
  \frac{\delta^{#1} #2}{{\delta #3}^{#1}}
  \fi
}
\newcommand{\fracDelta}[3][]{%
  \def\temp{#1} \ifx\temp\empty
